{
  "presets": [
    "@babel/env",
    ["@babel/typescript", { "jsxPragma": "h" }] // Add jsxPragma here
  ],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", { "pragma": "h" }],
    [
      "babel-plugin-jsx-pragmatic",
      {
        "module": "preact",
        "import": "h",
        "export": "h"
      }
    ]
  ]
}
